package loops6;
import java.util.Scanner;
public class PowerOfNumber {

	public static void main(String[] args) {
		 Scanner sc = new Scanner (System.in);
		 System.out.print("number x =");
		 int x = sc. nextInt();
		 System.out.print("number y =");
		 int y = sc .nextInt();
		 int result = 1;
		 for ( int i = 1;i<=y ;i++ ) {
			  result *=x;
					  
		 }
	System.out.println("x^y ="+ result);
	}
}

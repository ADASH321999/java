package loops2;
import java.util.Scanner;
public class factorial {
    
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Factorial of number=");
		int x = sc.nextInt();
		int factorial=1; 
		for (int i =1;i<=x;i++) {
			 factorial = i * factorial;
		}
         System.out.print (x+"!="+ factorial);
         
	}

}

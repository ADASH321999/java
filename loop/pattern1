            System.out.print( "Number of Stars in Length =" );package loops3;
import java.util.Scanner;
public class Patterns {
      
       
	public static void main(String[] args) {
		 Scanner sc = new Scanner (System.in);
            System.out.print( "Number of Stars in Length =" );
		    int n = sc.nextInt();
            System.out.print( "Number of Stars in Width =");
            int m = sc.nextInt();
		for ( int i =1;i<=n ;i++) {
			
			for (int j =1;j<=m;j++) {
				
				System.out.print(" * ");
		}
			
                System.out.println( );
	}
		if (m==n) {
        
		System.out.println( " This is a Square ");
}else {System.out.println(" This is a Rectangle");
}
	}
}
		    

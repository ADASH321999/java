package opertorsBitwise;
public class BitwisOperatorUse {

	public static void main(String[] args) {
    
     int a = 5;
     int b = 4;
     int c = a>>2;
     int d = b<<1;
     System.out.println(a&b);
     System.out.println(a|b);
     System.out.println(c);
     System.out.println(d); 
     
	}

}
 
